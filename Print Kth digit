#include <iostream>
#include <math.h>
using namespace std;

int main() {
	//code
	int test;
	cin>>test;
	for(int i=0;i<test;i++)
	{
	    int a;
	    int b;
	    int k;
	    int z;
	    cin>>a>>b>>k;
	    long num = pow(a,b);
	    for(int i=1;i<=k;i++)
	    {
	        if(i==k)
	            z = num%10;
	        else
	            num = num/10;
	    }
	   cout<<z<<endl;
	}
	return 0;
}
